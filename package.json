{
  "name": "flutter-i18n-helper",
  "type": "module",
  "displayName": "flutter-i18n-helper",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dart",
        "extensions": [
          ".dart"
        ],
        "aliases": [
          "Dart"
        ]
      }
    ],
    "files.associations": {
      "*.dart": "dart"
    },
    "commands": [
      {
        "command": "flutterI18nHelper.initTranslations",
        "title": "Flutter I18n Helper: Init Translations"
      },
      {
        "command": "flutterI18nHelper.uploadLocalTranslations",
        "title": "Flutter I18n Helper: Upload Local Translations"
      },
      {
        "command": "flutterI18nHelper.fetchCloudTranslations",
        "title": "Flutter I18n Helper: Fetch Cloud Translations"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/flutter-i18n-helper.git"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.9.0",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4"
  }
}
